<!-- Start Test Project Card -->
<div class="card-container">
  <div class="card-top learn">
    <span class="project-type">Volunteer</span>
   <div class="blur"></div>
  </div>
  <div class="content">
    <div class="details">
      <span> Active 4 days ago </span>
      <span>...</span>
    </div>
    <div class="title"> <%= @post.title %></div> <!-- tried to add this and got assigns error: <%#= @post.title %> -->
    <div class="summary"><%= truncate @post.body, length: 40, separator: " " %></div>
    <div class="done-so-far"><%= @post.done_so_far_one_title %></div>

    <!--<a href="#postModal" data-post-id="<%#= @post.id %>" class="btn">Read More</a>-->
    <!--s <span class="upvote-count", data-current-count="<%#= @post.upvotes_count.count %>">-->

    <a href="#postModal" id="postModalShow" data-post-id="<%= @post.id %>" class="btn">Read More</a>
     <%= link(" 📖  ", to: post_path(@conn, :show, @post), "data-tooltip": " Read ☺ ", "data-placement": "top", class: "btn btn-default btn-md btn-read-post unicode-lg") %>

    <span class="upvote-count", data-current-count="<%= @post.upvotes_count.count %>">

      <%= if @upvote do %>
        <%= link " #{@post.upvotes_count.count} ❤ ", to: post_upvote_path(@conn, :create, @post), "ujs-method": "delete", "ujs-remote": true, "ujs-eval": true, class: "btn btn-default btn-md unicode-lg upvote-btn upvote-delete-btn upvote-font" %>
      <% else %>
        <%= link(" #{@post.upvotes_count.count} ❤ ", to: post_upvote_path(@conn, :create, @post), "data-tooltip": "🚀💖", "data-placement": "top", "ujs-method": "post", "ujs-remote": true, "ujs-eval": true, class: "btn btn-default btn-md btn-edit-post unicode-lg upvote-btn upvote-add-btn upvote-font") %>
      <% end %>
    </span>
  </div>
</div>
